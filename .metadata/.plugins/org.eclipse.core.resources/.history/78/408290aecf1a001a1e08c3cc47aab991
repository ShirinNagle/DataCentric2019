package com.shops;



import javax.faces.bean.ManagedBean;

@ManagedBean
public class Store {
	
	//class variables
	private int storeId;
	private String storeName;
	private String founded;
	
	public Store() {}//no argument constructor as ManagedBean reqs it
	
	public Store(int storeId, String storeName, String founded) {//not sure if useful yet
		this.storeId = storeId;
		this.storeName = storeName;
		this.founded = founded;
	}
	
	public Store(String storeName, String founded) {//not sure if useful yet
		//this.storeId = storeId;
		this.storeName = storeName;
		this.founded = founded;
	}
	
	//Getters and setters for class variables
	public int getStoreId() {
		return storeId;
	}
	public void setStoreId(int storeId) {
		this.storeId = storeId;
	}
	public String getStoreName() {
		return storeName;
	}
	public void setStoreName(String storeName) {
		this.storeName = storeName;
	}
	public String getFounded() {
		return founded;
	}
	public void setFounded(String founded) {
		this.founded = founded;
	}
	

}//end of class
